<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.lemontree0802.mapper.AdministratorMapper">

    <resultMap id="administratorMap" type="com.lemontree0802.pojo.Administrator">
        <result column="created_at" property="createdAt" />
    </resultMap>

    <delete id="DeleteUser">
        DELETE FROM users
        WHERE id = #{userId};
    </delete>

    <delete id="DeleteMerchant">
        DELETE FROM merchants
        WHERE id = #{merchantId};
    </delete>

    <select id="selectAll" resultMap="administratorMap">
        SELECT * FROM administrators;
    </select>

</mapper>
